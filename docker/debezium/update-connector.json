{
  "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
  "database.hostname": "postgres",
  "tasks.max": "1",
  "group.id":"example-group",
  "database.port": "5432",
  "database.user": "postgres",
  "database.password": "postgres",
  "database.dbname": "postgres",
  "database.server.name":"dbserver1",
  "database.whitelist":"public",
  "slot.name": "example_debezium_slot",
  "table.include.list":"public.mc_transactions",
  "snapshot.select.statement.overrides":"public.transaction",
  "snapshot.select.statement.overrides.public.mc_transactions":"SELECT * FROM public.transaction",
  "topic.prefix": "mc_transactions",
  "tombstones.on.delete":"false",
  "value.converter.schemas.enable":"false",
  "key.converter.schemas.enable":"false",
  "key.converter": "org.apache.kafka.connect.storage.StringConverter",
  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
  "decimal.handling.mode" :"string",
  "time.precision.mode":"connect",
  "transforms.TimestampConverter.format":"yyyy-MM-dd HH:mm:ss.SSSSSS",
  "transforms": "unwrap, RenameField",
  "transforms.RenameField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
  "transforms.RenameField.renames": "name:firstName",
  "transforms.unwrap.type":"io.debezium.transforms.ExtractNewRecordState",
  "transforms.unwrap.topic.regex":"transaction"
